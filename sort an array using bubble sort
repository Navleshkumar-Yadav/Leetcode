class Solution {
    public int[] sortArray(int[] arr) {
        int n = arr.length;
        for(int x = 0 ; x<n-1 ; x++){
            for(int i = 0 ; i<n-1 ; i++){
                if(arr[i]>arr[i+1]){
                    int temp = arr[i];
                    arr[i] = arr[i+1];
                    arr[i+1] = temp;
                }
            }
        }
        return arr;
      
        
    }
}
