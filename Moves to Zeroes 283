class Solution {
    
    public void moveZeroes(int[] arr) {
        int noZ=0;
        int n = arr.length;
        for(int ele : arr){
            if(noZ==0) noZ++;

            
        }
        for(int x = 0 ; x<=noZ ; x++){
            for(int i = 0 ; i<n-1;i++){
                if(arr[i]==0){
                    int temp = arr[i];
                    arr[i] = arr[i+1];
                    arr[i+1] = temp;
                }
            }
        }

        
    }
}
